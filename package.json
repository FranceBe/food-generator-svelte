    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "5.0.3",
    "rollup-plugin-terser": "5.1.2",
    "svelte": "3.0.0"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "chalk": "4.0.0",
    "cors": "2.8.5",
    "express": "4.17.1",
    "nodemon": "2.0.4",
    "request": "2.88.2",
    "sirv-cli": "0.4.4"
  }
